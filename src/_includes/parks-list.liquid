<ul class="park-container">
  {% for park in parksList %}
    <li>
      <figure>
        <img src="{{ park.images[0].url }}" alt="{{ park.images[0].altText }}">
        <figcaption>{{ park.images[0].caption }}</figcaption>
      </figure>
      <h1>{{ park.fullName }}</h1>
      <p>{{ park.description }}</p>
    </li>
  {% endfor %}
</ul>